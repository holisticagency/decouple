    "license": "MIT",
    "type": "library",
    "keywords": [
        "clock",
        "system",
        "platform",
        "network",
        "pseudo-random",
        "number-generator",
        "decoupling",
        "interfaces"
    ],
    "authors": [
        {
            "name": "JamesRezo",
            "email": "james@rezo.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3"
    },
    "provide": {
        "psr/clock-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
